        }
        // Check file type
        const isValidType = acceptedTypes.some((type)=>{
            if (type.startsWith('.')) {
                return file.name.toLowerCase().endsWith(type.toLowerCase());
            }
            return file.type === type;
        });
        if (!isValidType) {
            return 'Please select a CSV file';
