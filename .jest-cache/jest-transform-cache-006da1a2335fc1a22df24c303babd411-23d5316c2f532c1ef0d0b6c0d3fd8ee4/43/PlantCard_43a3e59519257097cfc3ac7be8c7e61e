      /* istanbul ignore next */
      cov_qnqlzd1fv().f[18]++;
      cov_qnqlzd1fv().s[87]++;
      return setIsPressed(false);
    },
    onMouseLeave: () => {
      /* istanbul ignore next */
      cov_qnqlzd1fv().f[19]++;
      cov_qnqlzd1fv().s[88]++;
