      /* istanbul ignore next */
      (cov_qnqlzd1fv().b[31][0]++, daysUntilFertilizerDue !== null) &&
      /* istanbul ignore next */
      (cov_qnqlzd1fv().b[31][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
        className: `${config.text} text-neutral-600`,
        children: formatDays(daysUntilFertilizerDue)
      }))]
    });
  };
  // Get quick actions
  /* istanbul ignore next */
  cov_qnqlzd1fv().s[73]++;
  const getQuickActions = () => {
    /* istanbul ignore next */
    cov_qnqlzd1fv().f[12]++;
    cov_qnqlzd1fv().s[74]++;
    if (
    /* istanbul ignore next */
    (cov_qnqlzd1fv().b[33][0]++, !onCareAction) &&
    /* istanbul ignore next */
    (cov_qnqlzd1fv().b[33][1]++, !onEdit)) {
      /* istanbul ignore next */
      cov_qnqlzd1fv().b[32][0]++;
      cov_qnqlzd1fv().s[75]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_qnqlzd1fv().b[32][1]++;
    }
    cov_qnqlzd1fv().s[76]++;
    return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
