        cov_rq6e778m8().s[37]++;
        navigator.vibrate(pattern);
      } else
      /* istanbul ignore next */
      {
        cov_rq6e778m8().b[5][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_rq6e778m8().s[38]++;
      console.log('Vibration not supported:', error);
    }
  }, []));
  const isHapticSupported =
  /* istanbul ignore next */
  (cov_rq6e778m8().s[39]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_rq6e778m8().f[3]++;
    cov_rq6e778m8().s[40]++;
    if (typeof navigator === 'undefined') {
      /* istanbul ignore next */
      cov_rq6e778m8().b[7][0]++;
      cov_rq6e778m8().s[41]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_rq6e778m8().b[7][1]++;
    }
    cov_rq6e778m8().s[42]++;
    return /* istanbul ignore next */(cov_rq6e778m8().b[8][0]++, typeof navigator.vibrate === 'function') ||
    /* istanbul ignore next */
    (cov_rq6e778m8().b[8][1]++, 'hapticFeedback' in navigator);
