    (cov_u3lpf0q81().b[25][1]++, !isRefreshing) &&
    /* istanbul ignore next */
    (cov_u3lpf0q81().b[25][2]++, pullDistance > 0)) {
      /* istanbul ignore next */
      cov_u3lpf0q81().b[24][0]++;
      cov_u3lpf0q81().s[75]++;
      // Animate back to 0
      const animate = () => {
        /* istanbul ignore next */
        cov_u3lpf0q81().f[8]++;
        cov_u3lpf0q81().s[76]++;
        setPullDistance(prev => {
          /* istanbul ignore next */
          cov_u3lpf0q81().f[9]++;
          const newDistance =
          /* istanbul ignore next */
          (cov_u3lpf0q81().s[77]++, prev * 0.9);
          /* istanbul ignore next */
          cov_u3lpf0q81().s[78]++;
          if (newDistance < 1) {
            /* istanbul ignore next */
            cov_u3lpf0q81().b[26][0]++;
            cov_u3lpf0q81().s[79]++;
            return 0;
          } else
          /* istanbul ignore next */
          {
            cov_u3lpf0q81().b[26][1]++;
          }
          cov_u3lpf0q81().s[80]++;
          animationFrameRef.current = requestAnimationFrame(animate);
          /* istanbul ignore next */
          cov_u3lpf0q81().s[81]++;
          return newDistance;
        });
      };
      /* istanbul ignore next */
      cov_u3lpf0q81().s[82]++;
      animationFrameRef.current = requestAnimationFrame(animate);
    } else
    /* istanbul ignore next */
    {
      cov_u3lpf0q81().b[24][1]++;
    }
    cov_u3lpf0q81().s[83]++;
    return () => {
      /* istanbul ignore next */
      cov_u3lpf0q81().f[10]++;
      cov_u3lpf0q81().s[84]++;
      if (animationFrameRef.current) {
        /* istanbul ignore next */
        cov_u3lpf0q81().b[27][0]++;
        cov_u3lpf0q81().s[85]++;
        cancelAnimationFrame(animationFrameRef.current);
      } else
      /* istanbul ignore next */
      {
        cov_u3lpf0q81().b[27][1]++;
      }
    };
  }, [isPulling, isRefreshing, pullDistance]);
  /* istanbul ignore next */
  cov_u3lpf0q81().s[86]++;
  const getRefreshIndicatorStyle = () => {
    /* istanbul ignore next */
    cov_u3lpf0q81().f[11]++;
    const progress =
    /* istanbul ignore next */
    (cov_u3lpf0q81().s[87]++, Math.min(pullDistance / threshold, 1));
    const opacity =
    /* istanbul ignore next */
    (cov_u3lpf0q81().s[88]++, Math.min(progress * 2, 1));
    const scale =
    /* istanbul ignore next */
    (cov_u3lpf0q81().s[89]++, 0.5 + progress * 0.5);
    const rotation =
    /* istanbul ignore next */
    (cov_u3lpf0q81().s[90]++, progress * 180);
    /* istanbul ignore next */
    cov_u3lpf0q81().s[91]++;
    return {
      opacity,
      transform: `scale(${scale}) rotate(${rotation}deg)`,
      transition: isPulling ?
      /* istanbul ignore next */
      (cov_u3lpf0q81().b[28][0]++, 'none') :
      /* istanbul ignore next */
      (cov_u3lpf0q81().b[28][1]++, 'all 0.3s ease-out')
    };
  };
  /* istanbul ignore next */
  cov_u3lpf0q81().s[92]++;
  return {
    elementRef,
    isRefreshing,
    isPulling,
    pullDistance,
    progress: Math.min(pullDistance / threshold, 1),
    getRefreshIndicatorStyle
  };
}
