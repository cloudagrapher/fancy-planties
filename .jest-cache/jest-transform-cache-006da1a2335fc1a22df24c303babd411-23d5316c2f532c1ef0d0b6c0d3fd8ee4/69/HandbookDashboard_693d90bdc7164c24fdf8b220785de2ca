          cov_iyucxasu3().f[33]++;
          cov_iyucxasu3().s[95]++;
          return setShowCreateForm(true);
        },
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Plus, {
          className: "h-4 w-4"
        }), "Create Your First Guide"]
      }))]
    })), /*#__PURE__*/(0, _jsxruntime.jsx)(_CareGuideForm.default, {
      isOpen: showCreateForm,
      onClose: () => {
        /* istanbul ignore next */
        cov_iyucxasu3().f[34]++;
        cov_iyucxasu3().s[96]++;
        return setShowCreateForm(false);
      },
      onSubmit: handleCreateGuide
    })]
  });
