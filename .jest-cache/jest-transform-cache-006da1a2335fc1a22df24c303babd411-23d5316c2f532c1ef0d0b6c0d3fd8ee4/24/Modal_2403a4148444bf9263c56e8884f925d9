          onClick: () => {
            /* istanbul ignore next */
            cov_299ebzd118().f[15]++;
            cov_299ebzd118().s[37]++;
            return onTabChange(tab.id);
          },
          className: `tab ${activeTab === tab.id ?
          /* istanbul ignore next */
          (cov_299ebzd118().b[16][0]++, 'tab--active') :
          /* istanbul ignore next */
