        className: "plant-card-content",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "h-4 bg-gray-200 rounded mb-2"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "h-3 bg-gray-200 rounded mb-1"
