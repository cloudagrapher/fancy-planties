            /* istanbul ignore next */
            cov_rq6e778m8().b[3][0]++;
            cov_rq6e778m8().s[10]++;
            navigator.hapticFeedback.impactOccurred('light');
            /* istanbul ignore next */
            cov_rq6e778m8().s[11]++;
