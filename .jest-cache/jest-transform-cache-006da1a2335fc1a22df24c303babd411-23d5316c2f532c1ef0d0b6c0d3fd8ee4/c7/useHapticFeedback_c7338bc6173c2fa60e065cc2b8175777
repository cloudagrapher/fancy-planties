      } catch (error) {
        /* istanbul ignore next */
        cov_rq6e778m8().s[22]++;
        console.log('Haptic feedback not available:', error);
      }
    } else
    /* istanbul ignore next */
    {
      cov_rq6e778m8().b[2][1]++;
