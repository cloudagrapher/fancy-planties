function ModalWithTabs({
  tabs,
  activeTab,
  onTabChange,
  ...modalProps
}) {
  /* istanbul ignore next */
  cov_299ebzd118().f[12]++;
  const activeTabContent =
  /* istanbul ignore next */
  (cov_299ebzd118().s[33]++, tabs.find(tab => {
    /* istanbul ignore next */
    cov_299ebzd118().f[13]++;
    cov_299ebzd118().s[34]++;
