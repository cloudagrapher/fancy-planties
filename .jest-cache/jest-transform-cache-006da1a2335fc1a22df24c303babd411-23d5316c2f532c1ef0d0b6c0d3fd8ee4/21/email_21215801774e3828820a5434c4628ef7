    (cov_15fnqfwaj8().b[15][0]++, message.includes('network')) ||
    /* istanbul ignore next */
    (cov_15fnqfwaj8().b[15][1]++, message.includes('timeout'))) {
      /* istanbul ignore next */
      cov_15fnqfwaj8().b[14][0]++;
      cov_15fnqfwaj8().s[43]++;
      return 'NETWORK_ERROR';
    } else
    /* istanbul ignore next */
    {
      cov_15fnqfwaj8().b[14][1]++;
    }
    cov_15fnqfwaj8().s[44]++;
    return 'API_ERROR';
  }
