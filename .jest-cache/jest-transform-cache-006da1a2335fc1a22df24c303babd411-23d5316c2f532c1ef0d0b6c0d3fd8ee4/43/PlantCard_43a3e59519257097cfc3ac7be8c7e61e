        })]
      })]
    });
  } else
  /* istanbul ignore next */
  {
    cov_qnqlzd1fv().b[36][1]++;
  }
  cov_qnqlzd1fv().s[80]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    ref: swipeRef,
    role: "button",
    tabIndex: 0,
    className: `
        plant-card ${config.container} group
        ${isPressed ?
    /* istanbul ignore next */
