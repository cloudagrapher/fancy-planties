          /* istanbul ignore next */
          cov_qnqlzd1fv().b[26][0]++;
          cov_qnqlzd1fv().s[69]++;
          statusText = 'Due Soon';
        } else {
          /* istanbul ignore next */
          cov_qnqlzd1fv().b[26][1]++;
          cov_qnqlzd1fv().s[70]++;
          if (careStatus === 'unknown') {
            /* istanbul ignore next */
            cov_qnqlzd1fv().b[27][0]++;
            cov_qnqlzd1fv().s[71]++;
            statusText = 'No Schedule';
          } else
          /* istanbul ignore next */
          {
            cov_qnqlzd1fv().b[27][1]++;
          }
        }
      }
    }
    /* istanbul ignore next */
    cov_qnqlzd1fv().s[72]++;
