        /* istanbul ignore next */
        cov_2gjjkbydq5().s[52]++;
        body = await request.json();
        /* istanbul ignore next */
        cov_2gjjkbydq5().s[53]++;
        if (
        /* istanbul ignore next */
        (cov_2gjjkbydq5().b[19][0]++, !body) ||
        /* istanbul ignore next */
        (cov_2gjjkbydq5().b[19][1]++, Object.keys(body).length === 0)) {
          /* istanbul ignore next */
          cov_2gjjkbydq5().b[18][0]++;
          cov_2gjjkbydq5().s[54]++;
          return _server.NextResponse.json({
            success: false,
            error: 'Request body is required'
          }, {
            status: 400
          });
        } else
        /* istanbul ignore next */
        {
          cov_2gjjkbydq5().b[18][1]++;
        }
      } catch (jsonError) {
        /* istanbul ignore next */
        cov_2gjjkbydq5().s[55]++;
        return _server.NextResponse.json({
          success: false,
          error: 'Invalid JSON in request body'
        }, {
          status: 400
        });
      }
    }
    // Add user ID to the request body and convert date strings to Date objects
    const instanceData =
    /* istanbul ignore next */
    (cov_2gjjkbydq5().s[56]++, {
      ...body,
      userId: user.id,
      // Convert date strings to Date objects if they exist and are not empty
      lastFertilized:
      /* istanbul ignore next */
      (cov_2gjjkbydq5().b[21][0]++, body.lastFertilized) &&
      /* istanbul ignore next */
      (cov_2gjjkbydq5().b[21][1]++, body.lastFertilized !== '') ?
      /* istanbul ignore next */
      (cov_2gjjkbydq5().b[20][0]++, new Date(body.lastFertilized)) :
      /* istanbul ignore next */
      (cov_2gjjkbydq5().b[20][1]++, null),
      lastRepot:
      /* istanbul ignore next */
      (cov_2gjjkbydq5().b[23][0]++, body.lastRepot) &&
      /* istanbul ignore next */
      (cov_2gjjkbydq5().b[23][1]++, body.lastRepot !== '') ?
      /* istanbul ignore next */
      (cov_2gjjkbydq5().b[22][0]++, new Date(body.lastRepot)) :
      /* istanbul ignore next */
      (cov_2gjjkbydq5().b[22][1]++, null)
    });
    // Validate the plant instance data
    let validatedData;
    /* istanbul ignore next */
    cov_2gjjkbydq5().s[57]++;
    try {
      /* istanbul ignore next */
      cov_2gjjkbydq5().s[58]++;
      validatedData = _plantschemas.createPlantInstanceSchema.parse(instanceData);
    } catch (validationError) {
      /* istanbul ignore next */
      cov_2gjjkbydq5().s[59]++;
      if (validationError instanceof _zod.z.ZodError) {
        /* istanbul ignore next */
        cov_2gjjkbydq5().b[24][0]++;
        cov_2gjjkbydq5().s[60]++;
        return _server.NextResponse.json({
          success: false,
          error: 'Validation failed',
          details: validationError.issues
        }, {
          status: 400
        });
      } else
      /* istanbul ignore next */
      {
        cov_2gjjkbydq5().b[24][1]++;
      }
      cov_2gjjkbydq5().s[61]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Validation failed',
        details: validationError instanceof Error ?
        /* istanbul ignore next */
        (cov_2gjjkbydq5().b[25][0]++, validationError.message) :
        /* istanbul ignore next */
        (cov_2gjjkbydq5().b[25][1]++, 'Invalid data')
      }, {
        status: 400
      });
    }
    // Calculate initial fertilizer due date if schedule is provided
    /* istanbul ignore next */
    cov_2gjjkbydq5().s[62]++;
    if (
    /* istanbul ignore next */
    (cov_2gjjkbydq5().b[27][0]++, validatedData.fertilizerSchedule) &&
    /* istanbul ignore next */
    (cov_2gjjkbydq5().b[27][1]++, !validatedData.fertilizerDue)) {
      /* istanbul ignore next */
      cov_2gjjkbydq5().b[26][0]++;
      const now =
      /* istanbul ignore next */
      (cov_2gjjkbydq5().s[63]++, new Date());
      const scheduleMatch =
      /* istanbul ignore next */
      (cov_2gjjkbydq5().s[64]++, validatedData.fertilizerSchedule.match(/(\d+)\s*(day|week|month)s?/i));
      /* istanbul ignore next */
      cov_2gjjkbydq5().s[65]++;
      if (scheduleMatch) {
        /* istanbul ignore next */
        cov_2gjjkbydq5().b[28][0]++;
        const [, amount, unit] =
        /* istanbul ignore next */
        (cov_2gjjkbydq5().s[66]++, scheduleMatch);
        const dueDate =
        /* istanbul ignore next */
        (cov_2gjjkbydq5().s[67]++, new Date(now));
        /* istanbul ignore next */
        cov_2gjjkbydq5().s[68]++;
        switch (unit.toLowerCase()) {
          case 'day':
            /* istanbul ignore next */
            cov_2gjjkbydq5().b[29][0]++;
            cov_2gjjkbydq5().s[69]++;
            dueDate.setDate(dueDate.getDate() + parseInt(amount, 10));
            /* istanbul ignore next */
            cov_2gjjkbydq5().s[70]++;
            break;
          case 'week':
            /* istanbul ignore next */
            cov_2gjjkbydq5().b[29][1]++;
            cov_2gjjkbydq5().s[71]++;
            dueDate.setDate(dueDate.getDate() + parseInt(amount, 10) * 7);
            /* istanbul ignore next */
            cov_2gjjkbydq5().s[72]++;
            break;
          case 'month':
            /* istanbul ignore next */
            cov_2gjjkbydq5().b[29][2]++;
            cov_2gjjkbydq5().s[73]++;
            dueDate.setMonth(dueDate.getMonth() + parseInt(amount, 10));
            /* istanbul ignore next */
            cov_2gjjkbydq5().s[74]++;
            break;
        }
        /* istanbul ignore next */
        cov_2gjjkbydq5().s[75]++;
        validatedData.fertilizerDue = dueDate;
      } else
      /* istanbul ignore next */
      {
        cov_2gjjkbydq5().b[28][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_2gjjkbydq5().b[26][1]++;
    }
    // Create the plant instance
    const plantInstance =
    /* istanbul ignore next */
    (cov_2gjjkbydq5().s[76]++, await _plantinstances.PlantInstanceQueries.create(validatedData));
    // Get the enhanced plant instance with plant data
    const enhancedInstance =
    /* istanbul ignore next */
    (cov_2gjjkbydq5().s[77]++, await _plantinstances.PlantInstanceQueries.getEnhancedById(plantInstance.id));
    /* istanbul ignore next */
    cov_2gjjkbydq5().s[78]++;
    return _server.NextResponse.json({
      success: true,
      data: enhancedInstance
    }, {
      status: 201
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2gjjkbydq5().s[79]++;
    console.error('Failed to create plant instance:', error);
    /* istanbul ignore next */
    cov_2gjjkbydq5().s[80]++;
    if (
    /* istanbul ignore next */
    (cov_2gjjkbydq5().b[31][0]++, error instanceof Error) &&
    /* istanbul ignore next */
    (cov_2gjjkbydq5().b[31][1]++, error.message.includes('validation'))) {
      /* istanbul ignore next */
      cov_2gjjkbydq5().b[30][0]++;
      cov_2gjjkbydq5().s[81]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Invalid plant instance data',
        details: error.message
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_2gjjkbydq5().b[30][1]++;
    }
    cov_2gjjkbydq5().s[82]++;
    return _server.NextResponse.json({
      success: false,
      error: 'Failed to create plant instance'
    }, {
      status: 500
    });
  }
}
