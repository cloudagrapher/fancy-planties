      })
    }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "modal-body modal-body--no-padding",
      children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "p-6",
        children: activeTabContent
      })
    })]
  });
}
function ConfirmationModal({
  isOpen,
  onClose,
  onConfirm,
  title,
  message,
  confirmText =
  /* istanbul ignore next */
  (cov_299ebzd118().b[18][0]++, 'Confirm'),
  cancelText =
  /* istanbul ignore next */
  (cov_299ebzd118().b[19][0]++, 'Cancel'),
  variant =
  /* istanbul ignore next */
  (cov_299ebzd118().b[20][0]++, 'default'),
  isLoading =
  /* istanbul ignore next */
  (cov_299ebzd118().b[21][0]++, false)
}) {
  /* istanbul ignore next */
  cov_299ebzd118().f[16]++;
  cov_299ebzd118().s[38]++;
  const handleConfirm = () => {
    /* istanbul ignore next */
    cov_299ebzd118().f[17]++;
    cov_299ebzd118().s[39]++;
    onConfirm();
  };
  /* istanbul ignore next */
  cov_299ebzd118().s[40]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)(Modal, {
    isOpen: isOpen,
    onClose: onClose,
    title: title,
    closeOnBackdropClick: !isLoading,
    closeOnEscape: !isLoading,
    footer: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "flex gap-3 justify-end w-full",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
        onClick: onClose,
        disabled: isLoading,
        className: "btn btn--outline",
        children: cancelText
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
        onClick: handleConfirm,
        disabled: isLoading,
        className: `btn ${variant === 'danger' ?
        /* istanbul ignore next */
        (cov_299ebzd118().b[22][0]++, 'btn--danger') :
        /* istanbul ignore next */
        (cov_299ebzd118().b[22][1]++, 'btn--primary')} ${isLoading ?
        /* istanbul ignore next */
        (cov_299ebzd118().b[23][0]++, 'btn--loading') :
        /* istanbul ignore next */
        (cov_299ebzd118().b[23][1]++, '')}`,
        children: confirmText
      })]
    }),
    children: /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
      className: "text-gray-700",
      children: message
    })
  });
