      /* istanbul ignore next */
      (cov_qnqlzd1fv().b[35][0]++, onEdit) &&
      /* istanbul ignore next */
      (cov_qnqlzd1fv().b[35][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("button", {
        onClick: handleEdit,
        className: "btn btn--sm btn--secondary",
        title: "Edit plant",
        "aria-label": `Edit ${plant.displayName}`,
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          "aria-hidden": "true",
          children: "✏️"
        }), " Edit"]
      }))]
    });
  };
  // Show loading state if specified
  /* istanbul ignore next */
  cov_qnqlzd1fv().s[78]++;
  if (isLoading) {
    /* istanbul ignore next */
    cov_qnqlzd1fv().b[36][0]++;
    cov_qnqlzd1fv().s[79]++;
    return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
